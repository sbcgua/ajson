{
  "global": {
    "files": "/src/**/*.*",
    "skipGeneratedGatewayClasses": true,
    "skipGeneratedPersistentClasses": true,
    "skipGeneratedFunctionGroups": true
  },
  "dependencies": [{
    "url": "https://github.com/abaplint/deps",
    "folder": "/deps",
    "files": "/src/**/*.*"
  }],
  "syntax": {
    "version": "v702",
    "errorNamespace": "^(Z|Y|LCL_|TY_|LIF_)",
    "globalConstants": [],
    "globalMacros": []
  },
  "rules": {
    "align_parameters": false,
    "begin_single_include": true,
    "call_transaction_authority_check": true,
    "cds_legacy_view": true,
    "cds_parser_error": true,
    "local_testclass_consistency": true,
    "check_subrc": false,
    "classic_exceptions_overlap": true,
    "constant_classes": true,
    "cyclic_oo": true,
    "cyclomatic_complexity": false,
    "dangerous_statement": true,
    "db_operation_in_loop": true,
    "forbidden_pseudo_and_pragma": true,
    "function_module_recommendations": true,
    "identical_conditions": true,
    "identical_contents": false,
    "no_external_form_calls": true,
    "no_inline_in_optional_branches": true,
    "no_yoda_conditions": false,
    "prefer_is_not": false,
    "slow_parameter_passing": true,
    "static_call_via_instance": true,
    "superfluous_value": true,
    "sy_modification": false,
    "uncaught_exception": true,
    "unnecessary_chaining": false,
    "unnecessary_pragma": true,
    "unsecure_fae": false,
    "unused_ddic": true,
    "unused_methods": true,
    "use_bool_expression": true,
    "use_class_based_exceptions": true,
    "use_line_exists": true,
    "prefer_raise_exception_new": true,
    "prefer_xsdbool": true,
    "reduce_string_templates": true,
    "select_add_order_by": true,
    "select_performance": true,
    "unused_types": true,
    "nrob_consistency": true,
    "omit_parameter_name": false,
    "omit_preceding_zeros": true,
    "omit_receiving": true,
    "parser_702_chaining": true,
    "parser_missing_space": true,
    "pragma_style": true,
    "prefer_corresponding": true,
    "identical_descriptions": true,
    "intf_referencing_clas": true,
    "line_break_style": false,
    "many_parentheses": true,
    "max_one_method_parameter_per_line": true,
    "method_implemented_twice": true,
    "method_overwrites_builtin": false,
    "modify_only_own_db_tables": true,
    "names_no_dash": true,
    "no_aliases": false,
    "no_chained_assignment": true,
    "7bit_ascii": true,
    "abapdoc": false,
    "allowed_object_naming": true,
    "allowed_object_types": true,
    "ambiguous_statement": true,
    "avoid_use": {
      "exclude": [],
      "define": false,
      "testSeams": true,
      "statics": true,
      "defaultKey": false,
      "break": true,
      "describeLines": true
    },
    "begin_end_names": true,
    "chain_mainly_declarations": true,
    "check_abstract": true,
    "check_comments": false,
    "check_ddic": true,
    "check_include": true,
    "check_syntax": true,
    "check_text_elements": true,
    "check_transformation_exists": true,
    "class_attribute_names": {
      "exclude": [],
      "patternKind": "required",
      "ignoreNames": [],
      "ignorePatterns": [],
      "ignoreExceptions": true,
      "ignoreLocal": true,
      "ignoreInterfaces": false,
      "statics": "^G._.+$",
      "instance": "^M._.+$",
      "constants": ""
    },
    "cloud_types": true,
    "colon_missing_space": true,
    "commented_code": {
      "exclude": [],
      "allowIncludeInFugr": true
    },
    "constructor_visibility_public": true,
    "contains_tab": true,
    "definitions_top": false,
    "description_empty": true,
    "double_space": {
      "exclude": [],
      "keywords": true,
      "startParen": true,
      "endParen": true,
      "afterColon": true
    },
    "downport": true,
    "empty_line_in_statement": {
      "exclude": [],
      "allowChained": false
    },
    "empty_statement": true,
    "empty_structure": {
      "loop": true,
      "when": false,
      "try": true,
      "if": true,
      "while": true,
      "case": true,
      "select": true,
      "do": true,
      "at": true
    },
    "exit_or_check": true,
    "exporting": true,
    "forbidden_identifier": {
      "exclude": [],
      "check": []
    },
    "forbidden_void_type": {
      "exclude": [],
      "check": []
    },
    "form_tables_obsolete": true,
    "fully_type_constants": {
      "exclude": [],
      "checkData": true
    },
    "functional_writing": {
      "exclude": [],
      "ignoreExceptions": true
    },
    "global_class": true,
    "identical_form_names": true,
    "if_in_if": true,
    "implement_methods": true,
    "in_statement_indentation": false,
    "indentation": {
      "exclude": ["perf_test\\.prog\\.abap"],
      "ignoreExceptions": true,
      "alignTryCatch": true,
      "globalClassSkipFirst": false,
      "selectionScreenBlockIndentation": true,
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false
    },
    "inline_data_old_versions": true,
    "keep_single_parameter_on_one_line": {
      "exclude": [],
      "length": 120
    },
    "keyword_case": {
      "exclude": [],
      "style": "lower",
      "ignoreExceptions": true,
      "ignoreLowerClassImplmentationStatement": true,
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false,
      "ignoreFunctionModuleName": false,
      "ignoreGlobalClassBoundaries": true,
      "ignoreKeywords": []
    },
    "line_break_multiple_parameters": false,
    "line_length": {
      "exclude": [],
      "length": 120
    },
    "line_only_punc": {
      "exclude": [],
      "ignoreExceptions": true
    },
    "local_class_naming": {
      "exclude": [],
      "patternKind": "required",
      "ignoreNames": [],
      "ignorePatterns": [],
      "local": "^LCL_.+$",
      "exception": "^LCX_.+$",
      "test": "^LTCL_.+$"
    },
    "local_variable_names": {
      "exclude": [],
      "patternKind": "required",
      "ignoreNames": [],
      "ignorePatterns": [],
      "expectedData": "^L.*",
      "expectedConstant": "^LC_.+$",
      "expectedFS": "^(?!<L.?_).+" // No prefixes in field symbols please
    },
    "main_file_contents": true,
    "max_one_statement": true,
    "message_exists": true,
    "method_length": {
      "statements": 100,
      "errorWhenEmpty": true,
      "checkForms": true,
      "ignoreTestClasses": false
    },
    "method_parameter_names": {
      "exclude": [],
      "patternKind": "required",
      "ignoreNames": [],
      "ignorePatterns": [],
      "ignoreExceptions": true,
      "importing": "^I.?_.+$",
      "returning": "^R.?_.+$",
      "changing": "^C.?_.+$",
      "exporting": "^E.?_.+$"
    },
    "mix_returning": true,
    "msag_consistency": true,
    "nesting": {
      "exclude": [],
      "depth": 6
    },
    "newline_between_methods": {
      "exclude": [],
      "count": 3,
      "logic": "less"
    },
    "no_public_attributes": {
      "exclude": ["testclasses.abap$", "perf_test\\.prog\\.abap"],
      "ignoreTestClasses": false,
      "allowReadOnly": true
    },
    "object_naming": {
      "exclude": [],
      "patternKind": "required",
      "ignoreNames": [],
      "ignorePatterns": [],
      "clas": "^ZC(L|X)\\_",
      "intf": "^ZIF\\_",
      "prog": "^Z",
      "fugr": "^Z",
      "tabl": "^Z",
      "ttyp": "^Z",
      "dtel": "^Z",
      "doma": "^Z",
      "msag": "^Z",
      "tran": "^Z",
      "enqu": "^EZ",
      "auth": "^Z",
      "pinf": "^Z",
      "idoc": "^Z",
      "xslt": "^Z",
      "ssfo": "^Z",
      "ssst": "^Z",
      "shlp": "^Z"
    },
    "obsolete_statement": {
      "refresh": true,
      "compute": true,
      "parameter": true,
      "callTransformation": true,
      "communication": true,
      "exitFromSQL": true,
      "freeMemory": true,
      "occurences": true,
      "pack": true,
      "ranges": true,
      "regex": true,
      "selectWithoutInto": true,
      "sortByFS": true,
      "add": true,
      "subtract": true,
      "multiply": true,
      "move": true,
      "divide": true,
      "requested": true,
      "occurs": true,
      "setExtended": true,
      "withHeaderLine": true,
      "fieldSymbolStructure": true,
      "load": true,
      "typePools": true
    },
    "parser_error": true,
    "prefer_inline": true,
    "prefer_returning_to_exporting": true,
    "preferred_compare_operator": {
      "exclude": [],
      "badOperators": [
        "EQ",
        "><",
        "NE",
        "GE",
        "GT",
        "LT",
        "LE"
      ]
    },
    "prefix_is_current_class": {
      "exclude": [],
      "omitMeInstanceCalls": true
    },
    "release_idoc": true,
    "remove_descriptions": {
      "exclude": [],
      "ignoreExceptions": true,
      "ignoreWorkflow": true
    },
    "rfc_error_handling": true,
    "selection_screen_naming": {
      "exclude": [],
      "patternKind": "required",
      "ignoreNames": [],
      "ignorePatterns": [],
      "parameter": "^P_.+$",
      "selectOption": "^S_.+$"
    },
    "sequential_blank": {
      "exclude": [],
      "lines": 4
    },
    "short_case": {
      "exclude": [],
      "length": 1,
      "allow": []
    },
    "sicf_consistency": true,
    "space_before_colon": true,
    "space_before_dot": {
      "exclude": [],
      "ignoreGlobalDefinition": true,
      "ignoreExceptions": true
    },
    "sql_escape_host_variables": true,
    "start_at_tab": true,
    "superclass_final": true,
    "tabl_enhancement_category": true,
    "try_without_catch": true,
    "type_form_parameters": true,
    "types_naming": {
      "exclude": [],
      "pattern": "^(TY|T.{1,2})_.+$"
    },
    "unknown_types": true,
    "unreachable_code": true,
    "unused_variables": true,
    "use_new": true,
    "when_others_last": true,
    "whitespace_end": true,
    "xml_consistency": true
  }
}
